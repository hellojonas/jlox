fun make_adder(n) {
  fun adder(i) {
    return n +i;
  }
  return adder;
}

fun quot(a, b) {
	return a / b;
}

fun sub(a, b) {
	return a - b;
}

fun mul(a, b) {
	// multiplication
	return a * b;
}

var res = quot(sub(10, 2), mul(2, 2));

print res;
